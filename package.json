{
  "name": "styled-reset",
  "description": "Eric Meyer's Reset CSS for styled-components",
  "version": "2.0.8",
  "author": {
    "email": "zac@zacanger.com",
    "name": "Zac Anger",
    "url": "http://zacanger.com"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "index.d.ts",
    "lib"
  ],
  "scripts": {
    "build": "npm run clean && babel src/ -d lib/",
    "clean": "rm -rf lib && mkdir -p lib",
    "postbuild": "flow-copy-source src lib",
    "postinstall": "opencollective-postinstall",
    "prebuild": "npm t && sortpack",
    "preversion": "npm run build",
    "test": "run-s -s test:lint test:flow",
    "test:flow": "flow check",
    "test:lint": "eslint -c .eslintrc.json src"
  },
  "homepage": "https://github.com/zacanger/styled-reset#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/styled-reset.git"
  },
  "bugs": "https://github.com/zacanger/styled-reset/issues",
  "keywords": [
    "css",
    "reset",
    "reset.css",
    "styled-components"
  ],
  "dependencies": {
    "opencollective": "^1.0.3",
    "opencollective-postinstall": "2.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/plugin-transform-modules-commonjs": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "babel-eslint": "10.0.1",
    "eslint": "5.16.0",
    "eslint-config-zacanger": "3.4.2",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-unicorn": "8.0.2",
    "flow-bin": "0.97.0",
    "flow-copy-source": "2.0.3",
    "npm-run-all": "4.1.5",
    "sortpack": "1.1.4",
    "styled-components": "4.2.0"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/styled-reset"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "peerDependencies": {
    "styled-components": ">=4.0.0"
  },
  "typings": "index.d.ts"
}
